<h1>Listing writes</h1>
<p id="notice"><%= notice %></p>
<table>
  <tr>
    <th>Person</th>
    <th>Article</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @writes.each do |write| %>
  <tr>
    <td><%= write.person_id %></td>
    <td><%= write.article_id %></td>
    <td><%= link_to 'Show', write %></td>
    <td><%= link_to 'Edit', edit_write_path(write) %></td>
    <td><%= link_to 'Destroy', write, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

